{
  "name": "telefonica-github_actions_runner",
  "version": "0.10.0",
  "author": "Shimon Ohayon",
  "summary": "Module to configure our GitHub Actions runner on Debian hosts",
  "license": "Apache-2.0",
  "source": "https://github.com/Telefonica/puppet-github-actions-runner",
  "project_page": "https://github.com/Telefonica/puppet-github-actions-runner",
  "issues_url": "https://github.com/Telefonica/puppet-github-actions-runner/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0 < 10.0.0"
    },
    {
      "name": "puppet-systemd",
      "version_requirement": ">= 2.7.0 <= 5.2.0"
    },
    {
      "name": "puppet-archive",
      "version_requirement": ">= 1.1.2 < 5.0.0"
    }
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
